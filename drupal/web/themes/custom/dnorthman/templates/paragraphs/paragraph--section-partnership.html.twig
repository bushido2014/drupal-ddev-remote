<section class="partnership">
<div class="container">
<h2 class="section-title">Our Partnership</h2>
<div class="partnership-logos d-flex">
  {% for media in paragraph.field_image_partnership %}
    {% set file = media.entity.field_media_image.entity %}
    <figure class="partner-logo">
      <img 
        src="{{ file_url(file.uri.value) }}"
        alt="{{ file.field_media_image.alt }}"
        loading="lazy"
      >
    </figure>
  {% endfor %}
</div>
</div>
</section>