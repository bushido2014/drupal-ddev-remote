<div{{ attributes.addClass('link-simple-wrapper') }}>
  {% if not label_hidden %}
    <h3{{ title_attributes }}>{{ label }}</h3>
  {% endif %}

  {% for item in items %}
    <a href="{{ item.content['#url'] }}" class="btn">
      {{ item.content['#title'] }} <i class="fa-solid fa-arrow-right"></i>
    </a>
  {% endfor %}
</div>