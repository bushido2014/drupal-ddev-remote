<section class="bg-page-title" style="background-image: url('{{ base_path ~ directory }}/img/bg-title.png');">
    <div class="container d-flex">
      <h1 class="page-title">Lawyer</h1>
      </div>
  </section>
    
<section class="lawyer"> 

<div class="container">
<div class="lawyer-list d-grid">
  {% for row in rows %}
    <div class="lawyer-item">
      {{ row.content }}
    </div>
  {% endfor %}
</div>
</div>

</section>
<section class="lawyer"> 

 <div class="container">
    <div class="lawyer-list d-grid">
      {% for row in rows %}
        <div class="lawyer-item">
          <div class="lawyer-image">
            {{ row.content.field_image }}
          </div>
          <div class="lawyer-name">
            {{ row.content.title }}
          </div>
          <div class="lawyer-occupation">
            {{ row.content.field_occupation }}
          </div>
        </div>
      {% endfor %}
    </div>
  </div>
</section>